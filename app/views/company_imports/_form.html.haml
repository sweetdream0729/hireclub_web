= form_for @company, url: company_imports_path do |f|
  .form-group
    = f.label "Name"
    = f.text_field :name, class: 'form-control', placeholder: "HireClub", autocomplete: "off"
  .form-group
    = f.label "Facebook URL"
    = f.url_field :facebook_url, class: 'form-control'
  = f.submit 'Import', class: 'btn btn-primary btn-lg'
