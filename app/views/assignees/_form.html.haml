= form_for(Assignee.new,remote: true, html: {class: "form-horizontal" }) do |f|
  = f.hidden_field :appointment_id, value: appointment.id
  .form-group
    = select_tag :users,[],{ multiple: true, class: "autocomplete_assignee"}
    .form-control-feedback
      
  = f.submit "Assign", class: 'btn btn-primary btn-sm', data: {disable_with: "Please wait..."}
  = link_to "Assign me", assign_me_assignee_path(appointment_id: appointment.id), remote: true, class: 'btn btn-outline-primary btn-sm',data: {disable_with: "Please wait..."}